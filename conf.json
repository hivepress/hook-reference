{
  "opts": {
    "destination": "hookdocs",
    "template": "node_modules/wp-hookdoc/template",
    "recurse": true,
    "readme": "./README.md"
  },
  "source": {
    "include": [ "./hivepress" ],
    "includePattern": ".+\\.(php)?$"
  },
  "plugins": [
    "node_modules/wp-hookdoc/plugin",
    "plugins/markdown"
  ]
}
